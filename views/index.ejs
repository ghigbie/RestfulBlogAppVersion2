<% include partials/header %>

<div class="container">
    <h1>Index Page</h1>
    
    <% blogs.forEach(function(blog){ %>
        <div>
          <h1> <%= blog.title %> </h1>
          <img src="<%= blog.image %>" >
          <span> <%= blog.created %> </span>
          <p> <%= blog.body %> </p>
        </div>
    <% }) %>
    
</div>


<% include partials/footer %>